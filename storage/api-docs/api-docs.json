{
    "openapi": "3.0.0",
    "info": {
        "title": "API  de OZ STREAM",
        "version": "0.0.1"
    },
    "servers": [
        {
            "url": "http://localhost:8001/",
            "description": "OZ STREAM "
        }
    ],
    "paths": {
        "/api/upload/media": {
            "post": {
                "tags": [
                    "Upload Media"
                ],
                "summary": "Upload the media",
                "description": "return an url",
                "operationId": "uploadMedia",
                "requestBody": {
                    "required": true,
                    "content": {
                        "multipart/form-data": {
                            "schema": {
                                "required": [
                                    "media"
                                ],
                                "properties": {
                                    "media_id": {
                                        "type": "string"
                                    },
                                    "serie_id": {
                                        "type": "string"
                                    },
                                    "isFilmBande": {
                                        "type": "string"
                                    },
                                    "media": {
                                        "type": "file"
                                    }
                                },
                                "type": "object"
                            }
                        },
                        "application/json": {
                            "schema": {}
                        }
                    }
                },
                "responses": {
                    "201": {
                        "description": "Successful operation"
                    },
                    "400": {
                        "description": "Bad Request"
                    },
                    "401": {
                        "description": "Unauthenticated"
                    },
                    "403": {
                        "description": "Forbidden"
                    }
                }
            }
        }
    },
    "components": {
        "securitySchemes": {
            "bearerAuth": {
                "type": "http",
                "name": "bearerAuth",
                "in": "header",
                "bearerFormat": "JWT",
                "scheme": "bearer"
            }
        }
    },
    "security": [
        []
    ]
}